<launch>
  
  <node name = "limit_reader"
        type = "relay.py"
        pkg = "necst_ros3">
    <param name = "config_file"
           value = "$(find necst_ros3)/relay_conf/limit_reader.conf" />
  </node>

  <node name = "position_reader_m2"
        type = "relay.py"
        pkg = "necst_ros3">
    <param name = "config_file"
           value = "$(find necst_ros3)/relay_conf/position_reader_m2.conf" />
  </node>

  <node name = "position_commander_m2"
        type = "relay.py"
        pkg = "necst_ros3">
    <param name = "config_file"
           value = "$(find necst_ros3)/relay_conf/position_commander_m2.conf" />
  </node>

</launch>
